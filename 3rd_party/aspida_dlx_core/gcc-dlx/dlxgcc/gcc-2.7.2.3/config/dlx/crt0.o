;;;
;;; Most of the traps are called in files from libtraps.
.text
.align 2
.proc ___init
	lhi  r29, 0xFFFF
	addi r29, r0, 0xFFFC
	add  r30, r29, r0
	jal _main
	nop
.endproc ___init

.text
.align 2
.proc _exit
.global _exit
_exit:
	trap	0x300
	jr	r31
	nop
.endproc _exit

; for the benefit of gcc.
.proc ___main
.global ___main
___main:
	jr	r31
	nop
.endproc ___main
